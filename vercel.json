{
    "version": 2,
    "name": "music-album",
    "builds": [
       { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [
       { "src": "/(.*)", "dest": "/index.js" },
       { "src": "/dish", "dest": "/dish.route.js" },
       { "src": "/user", "dest": "/user.route.js" }
    ]
 }
  