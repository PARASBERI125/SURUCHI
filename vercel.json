{
    "version": 2,
    "name": "music-album",
    "builds": [
       { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [
       { "src": "/(.*)", "dest": "/index.js" },
       { "src": "/api/dish", "dest": "/api/dish.js" },
       { "src": "/api/user", "dest": "/api/user.js" }
    ]
 }
  